{
  "name": "pp-breadcrumbs-app",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build pp-breadcrumbs && ng serve",
    "build:lib": "ng build pp-breadcrumbs",
    "copy-license": "copy .\\LICENSE .\\dist\\pp-breadcrumbs",
    "copy-readme": "copy .\\README.md .\\dist\\pp-breadcrumbs",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/pp-breadcrumbs && npm pack",
    "package": "npm run build:lib && npm run copy-files && npm run npm_pack",
    "publish": "npm run build:lib && npm run copy-files && npm publish ./dist/pp-breadcrumbs",
    "start:lib": "ng build pp-breadcrumbs --watch",
    "build": "npm run build:lib && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.7",
    "@angular/common": "~10.0.7",
    "@angular/compiler": "~10.0.7",
    "@angular/core": "~10.0.7",
    "@angular/forms": "~10.0.7",
    "@angular/platform-browser": "~10.0.7",
    "@angular/platform-browser-dynamic": "~10.0.7",
    "@angular/router": "~10.0.7",
    "core-js": "^2.5.4",
    "lodash": "^4.17.11",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "~10.0.7",
    "@angular/language-service": "~10.0.7",
    "@types/node": "^12.11.1",
    "@types/jasmine": "^3.5.7",
    "@types/jasminewd2": "~2.0.6",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "^8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
