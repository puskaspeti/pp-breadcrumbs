{
  "name": "pp-breadcrumbs-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng build pp-breadcrumbs && ng serve",
    "build:lib": "ng build pp-breadcrumbs",
    "copy-license": "copy .\\LICENSE .\\dist\\pp-breadcrumbs",
    "copy-readme": "copy .\\README.md .\\dist\\pp-breadcrumbs",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/pp-breadcrumbs && npm pack",
    "package": "npm run build:lib && npm run copy-files && npm run npm_pack",
    "publish": "npm run build:lib && npm run copy-files && npm publish ./dist/pp-breadcrumbs",
    "start:lib": "ng build pp-breadcrumbs --watch",
    "build": "npm run build:lib && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "core-js": "^2.5.4",
    "lodash": "^4.17.11",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
